    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55e392518650_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55e3924cf190_0;
    %flag_set/vec4 8;
    %jmp/0 T_0.2, 8;
    %pushi/vec4 1, 0, 2;
    %jmp/1 T_0.3, 8;
T_0.2 ; End of true expr.
    %pushi/vec4 0, 0, 2;
    %jmp/0 T_0.3, 8;
 ; End of false expr.
    %blend;
T_0.3;
    %pad/s 1;
    %store/vec4 v0x55e392518650_0, 0, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55e392505cf0;
T_1 ;
    %vpi_call 2 14 "$dumpfile", "edge.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55e392505cf0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e392518b20_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e392518890_0, 0, 1;
